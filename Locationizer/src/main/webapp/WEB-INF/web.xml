<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetImageServlet</servlet-name>
  	<display-name>GetImageServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.GetImageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SubmitImageServlet</servlet-name>
  	<display-name>SubmitImageServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlet.SubmitImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetImageServlet</servlet-name>
  	<url-pattern>/getimage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubmitImageServlet</servlet-name>
  	<url-pattern>/submitimage</url-pattern>
  </servlet-mapping>
</web-app>
